{
  "name": "iznik",
  "version": "1.0.0",
  "description": "Iznik is a platform for online reuse of unwanted items.  The development has been funded by Freegle for use in the UK,  but it is an open source platform which can be used or adapted by others.",
  "scripts": {
    "clean": "rimraf dist",
    "build:fd": "npm run clean && cross-env NODE_ENV=production BASE_URL=https://www.ilovefreegle.org webpack --config dev/webpack.productionfd.config.js",
    "build:mt": "npm run clean && cross-env NODE_ENV=production BASE_URL=https://modtools.org webpack --config dev/webpack.productionmt.config.js",
    "build:devfd": "npm run clean && cross-env NODE_ENV=production BASE_URL=https://dev.ilovefreegle.org webpack --config dev/webpack.productionfd.config.js",
    "build:devmt": "npm run clean && cross-env NODE_ENV=production BASE_URL=https://dev.modtools.org webpack --config dev/webpack.productionmt.config.js",
    "build:localfd": "npm run clean && cross-env NODE_ENV=production BASE_URL=http://localhost:3000 webpack --config dev/webpack.productionfd.config.js",
    "build:localfd:watch": "npm run clean && cross-env NODE_ENV=production BASE_URL=http://localhost:3000 webpack --config dev/webpack.productionfd.config.js --watch",
    "build:localmt": "npm run clean && cross-env NODE_ENV=production BASE_URL=http://localhost:3000 webpack --config dev/webpack.productionmt.config.js",
    "build:localmt:watch": "npm run clean && cross-env NODE_ENV=production BASE_URL=http://localhost:3000 webpack --config dev/webpack.productionmt.config.js --watch",
    "serve:fd": "node dev/serverfd.js --use-dist",
    "dev:fd": "node dev/serverfd.js",
    "serve:mt": "node dev/servermt.js --use-dist",
    "dev:mt": "node dev/servermt.js",
    "lint": "eslint .",
    "format": "prettier --single-quote --write dev/**.js tests/**.js",
    "test:e2e": "node tests/e2e/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Freegle/iznik.git"
  },
